{{#each team as |one_team|}}
  {{log one_team}}
  <section data-id="{{one_team.id}}">
    <h6><span class="wordRed">Id: </span>{{one_team.id}}</h6>
    <h6><span class="wordRed">Name: </span>{{limit one_team.name 15}}</h6>
    <h6 class="wordRed">Date:</h6>
    <h5>
      {{#if one_team.create_date}}
        {{one_team.create_date}}
      {{else}}
        No date provided.
      {{/if}}
    </h5>
    <h6 class="wordRed">Comment:</h6>
    <h5>
      {{#if one_team.comment}}
        {{one_team.comment}}
      {{else}}
        No comment provided.
      {{/if}}
    </h5>
    -----------------------------

  </section>

{{/each}}
